{
  "name": "go-starter-kit",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "babel-node src/server",
    "eslint": "eslint --ext .js --ext .jsx src",
    "relay": "relay-compiler --src ./src --schema ./src/schema.graphql  --extensions js jsx"
  },
  "author": "olebedev",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-css-modules": "^3.4.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-react": "^7.7.0",
    "expose-loader": "^0.7.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "farce": "^0.2.6",
    "file-loader": "^0.11.1",
    "found": ">=0.3.8",
    "found-relay": "^0.3.0-alpha.12",
    "graphql-import": "^0.5.2",
    "graphql-tools": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "node-sass": "^4.9.0",
    "normalize.css": "^6.0.0",
    "postcss-functions": "^2.1.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "proxy-middleware": "^0.15.0",
    "ramda": "^0.25.0",
    "react": "16",
    "react-dom": "16",
    "react-helmet": "^5.2.0",
    "react-relay": "^1.6.0",
    "reactstrap": "^5.0.0",
    "relay-runtime": "^1.6.0",
    "sass-loader": "^7.0.1",
    "serialize-javascript": "^1.5.0",
    "style-loader": "^0.21.0",
    "url-loader": "^0.5.8",
    "uuid": "^3.2.1",
    "webpack": "4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.18.0",
    "when": "^3.7.8"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0",
      "flow"
    ],
    "plugins": [
      [
        "relay",
        {
          "schema": "./src/schema.graphql"
        }
      ]
    ],
    "env": {
      "dev": {
        "presets": [
          "react-hmre"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "never"
      ],
      "no-unreachable": "error",
      "no-unexpected-multiline": "error",
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "multiline-comment-style": [
        "error",
        "separate-lines"
      ],
      "function-paren-newline": [
        "error",
        "consistent"
      ],
      "curly": [
        "error",
        "multi",
        "consistent"
      ]
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "jest",
      "flowtype"
    ],
    "settings": {
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      },
      "import/resolver": {
        "node": {
          "paths": [
            "client"
          ],
          "extensions": [
            "",
            ".js",
            ".jsx",
            ".scss",
            ".css"
          ]
        }
      }
    }
  },
  "main": "index.js",
  "repository": "git@github.com:olebedev/go-starter-kit.git",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-relay": "^1.5.0",
    "babel-preset-flow": "^6.23.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "flow-bin": "^0.69.0",
    "graphql": "^0.13.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "react-test-renderer": "16",
    "relay-compiler": "^1.6.0",
    "webpack-cli": "^2.0.15"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|scss|less)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  }
}
